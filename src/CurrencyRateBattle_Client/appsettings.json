{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "WebServer": {
    "BaseURL": "https://localhost:7255", //"https://gorest.co.in/public/v2/"
    "RoomsURL": "api/rooms/get-rooms", //"getRooms"
    "GetBalanceURL": "api/account/get-balance"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/clientapp-.log"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
